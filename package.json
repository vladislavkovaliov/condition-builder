{
  "name": "wi-condition-builder",
  "version": "0.0.0-development",
  "private": false,
   "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup",
    "assert": "node --experimental-strip-types src/__tests__/index.test.ts ",
    "commit": "cz",
    "semantic-release": "semantic-release",
    "prepare": "npm run build",
    "format": "npx prettier . --write",
    "test": "vitest --run --maxConcurrency 1"
  },
  "author": "Vlad (v.v.kovaliov@gmail.com)",
  "license": "MIT",
  "keywords": [
    "condition",
    "builder"
  ],
  "description": "",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladislavkovaliov/condition-builder.git"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "semantic-release": "^24.2.6",
    "tsup": "^8.5.0"
  }
}
